Date=19      
Month=10        
Year=2025     
expiremessage="Script expired Dm @xDxO2 . New version available"

--Do not change below code－－ＷＡＲＮＩＮＧ ＤＯ ＮＯＴ ＣＨＡＮＧＥ ＢＥＬＯＷ ＣＯＤＥ 
function check(t) 
if t<10 then t="0"..t end
return t
end
expiredate=Year.. check(Month).. check(Date)
date=gg.makeRequest("http://www.guimp.com").headers["Date" ][1]  --guimp.com is smallest webpage so takes less time for loading 
month={"Jan", "Feb", "Mar", "Apr", "May", "Jun", "Jul", "Aug", "Sep", "Oct", "Nov", "Dec"}
for i=1,12 do
 if month[i]==string.sub(date,9, 11) then
      if i<10 then
       i="0"..i 
      end 
 currentdate=string.sub(date,13, 16)..i..string.sub(date,6, 7)  
 end
end
if tonumber(currentdate)>=tonumber(expiredate) then gg.alert(expiremessage) os.exit() end

gg.toast("🐍 SCRIPT OWNER! @xdxo2 ")

local state = {
    antenna = false,
    player_speed = false,
    magic_bodyshot = false,
}

local saved = {
    antenna = {},
    player_speed = {},
}

local function patchHex(offset, hex1, hex2)
    local ranges = gg.getRangesList("libil2cpp.so")
    if #ranges < 2 then
        gg.toast(" LIBIL2CPP RANGE NOT FOUND")
        return
    end
    local lib = ranges[2].start
    local addr1 = lib + offset
    gg.setValues({
        {address=addr1, value=hex1, flags=gg.TYPE_DWORD},
        {address=addr1+4, value=hex2, flags=gg.TYPE_DWORD}
    })
end

function turnOnNoAmmo()
    gg.toast("Unavailable")
end

function turnOffNoAmmo()
    gg.toast("Unavailable")
end

function turnOnNoRecoil()
    gg.toast("Unavailable")
end

function turnOffNoRecoil()
    gg.toast("Unavailable")
end

function toggleAntenna()
    state.antenna = not state.antenna
    gg.setRanges(gg.REGION_ANONYMOUS)
    if state.antenna then
        gg.searchNumber("-1.69093072414", gg.TYPE_FLOAT)
        saved.antenna = gg.getResults(1000)
        for i,v in ipairs(saved.antenna) do v.value = 999,999,999,987 end
        gg.setValues(saved.antenna)
        gg.clearResults()
        gg.toast(" 😱Antenna ON ")
    else
        for i,v in ipairs(saved.antenna) do v.value = -1.69093072414 end
        gg.setValues(saved.antenna)
        gg.clearResults()
        gg.toast(" 🥱Antenna OFF ")
    end
end

function togglePlayerSpeed()
    state.player_speed = not state.player_speed
    gg.setRanges(gg.REGION_ANONYMOUS + gg.REGION_C_DATA)
    if state.player_speed then
        gg.searchNumber("7", gg.TYPE_FLOAT)
        saved.player_speed = gg.getResults(600)
        for i,v in ipairs(saved.player_speed) do v.value = 15 end
        gg.setValues(saved.player_speed)
        gg.clearResults()
        gg.toast(" 🤬Player Speed ON ")
    else
        for i,v in ipairs(saved.player_speed) do v.value = 7 end
        gg.setValues(saved.player_speed)
        gg.clearResults()
        gg.toast(" 😔Player Speed OFF ")
    end
end

function mainMenu()
    local menu = gg.choice({
        (state.antenna and "📡 ANTENNA [ ON]") or "📡 ANTENNA [ OFF]",
        (state.player_speed and "🏃 PLAYER SPEED [ ON]") or "🏃 PLAYER SPEED [ OFF]",
         
          "📎 EXIT"
    }, nil, "👁️‍🗨️ (HACKERSEYE) OWNER @xDxO2(🐍) Premium")

    if menu == 1 then toggleAntenna() end
    if menu == 2 then togglePlayerSpeed() end

    if menu == 3 then os.exit() end
end

while true do
    if gg.isVisible(true) then
        gg.setVisible(false)
        mainMenu()
    end
    gg.sleep(100)
end

